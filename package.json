{
  "name": "mslearn-azure-terraform-yeoman",
  "version": "0.1.0",
  "description": "MSLearn Azure Terraform Yeoman example",
  "main": "index.js",
  "scripts": {
    "npm:install:global": "npm install --location=global bower yo generator-az-terra-module",
    "bower:update": "bower update",
    "prebower:install": "npm run bower:update",
    "bower:install": "bower install",
    "gem:update:system": "gem update --system",
    "gem:install": "gem install bundler",
    "bundle:install": "bundle install",
    "rake:build": "rake build",
    "setup": "npm run gem:install && npm run bundle:install & npm run npm:install:global",
    "postinstall": "npm run bower:install",
    "docker:compose:build": "docker compose build",
    "docker:compose:up": "docker compose up",
    "docker:run:bundler:bash": "docker run -it mslearn-azure-terraform-yeoman-bundler bash",
    "test": "exit 0"
  },
  "repository": {
    "type": "git",
    "url": "git+git@github.com:percebus/mslearn-azure-terraform-yeoman.git"
  },
